}\r\n    };\r\n    dispatch({\r\n      type: \"CARD_REFRESHED\",\r\n    });\r\n    loadParentCards();\r\n  }, [state.refreshCard]);\r\n  return (\r\n    <div className=\"container-fluid\">\r\n      <div style={{ paddingTop: 24 }}>\r\n        <div\r\n          className=\"d-flex justify-content-between\"\r\n          style={{\r\n            justifyContent: \"center\",\r\n            alignContent: \"center\",\r\n            alignItems: \"center\",\r\n          }}\r\n        >\r\n          <ContentRow>\r\n            <FaUserAlt size={24} />\r\n            <p\r\n              style={{\r\n                marginLeft: \"20px\",\r\n                fontWeight: \"bolder\",\r\n              }}\r\n            >\r\n              Personal Boards\r\n            </p>\r\n          </ContentRow>\r\n          <Button variant=\"none\" onClick={() => setShowAdd(true)}>\r\n            <ContentRow>\r\n              <p style={{ marginRight: \"20px\", fontWeight: \"bolder\" }}>\r\n                Add new activity\r\n              </p>\r\n              <div>\r\n                <IoMdAddCircle size={24} />\r\n              </div>\r\n            </ContentRow>\r\n          </Button>\r\n          <AddParentCard\r\n            show={showAdd}\r\n            onHide={() => setShowAdd(false)}\r\n            uploadBy={state.user.id}\r\n          />\r\n        </div>\r\n        {loading || !parenCards ? (\r\n          <PageLoading />\r\n        ) : parenCards.length === 0 ? (\r\n          <ContentCenter>\r\n            <ContentColumn>\r\n              <AiFillWarning size={72} />\r\n              <h1\r\n                style={{\r\n                  justifyContent: \"center\",\r\n                  alignItems: \"center\",\r\n                  alignContent: \"center\",\r\n                }}\r\n              >\r\n                You don't have any activities\r\n              </h1>\r\n            </ContentColumn>\r\n          </ContentCenter>\r\n        ) : (\r\n          <div className=\"row\">\r\n          